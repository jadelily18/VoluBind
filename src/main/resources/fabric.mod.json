{
  "schemaVersion": 1,
  "id": "volubind",
  "version": "${version}",

  "name": "VoluBind",
  "description": "Allows creation of keybinds to toggle in-game volume.",
  "authors": [
    "jadelily18"
  ],
  "contact": {
    "sources": "https://github.com/jadelily18/VoluBind"
  },

  "license": "CC0-1.0",
  "icon": "assets/volubind/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "com.lilydev.volubind.Volubind"
    ],
    "client": [
      "com.lilydev.volubind.VolubindClient"
    ],
    "modmenu": [
      "com.lilydev.volubind.VolubindModMenuApiImpl"
    ]
  },
  "mixins": [
    "volubind.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17",
    "cloth-config": ">=7.0.69"
  },
  "suggests": {
    "modmenu": ">=4.0.0"
  },
  "custom": { }
}
